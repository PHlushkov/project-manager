{
  "name": "project-manager",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:skip-check": "vite build",
    "preview": "vite preview",
    "mock": "json-server --watch db.json --port 3001",
    "start": "concurrently \"npm run dev\" \"npm run mock\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,vue,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,vue,js,jsx,json,css,scss,md}\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.87.1",
    "@tanstack/vue-table": "^8.21.3",
    "axios": "^1.13.2",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.24",
    "vue-draggable-next": "^2.3.0",
    "vue-router": "^4.6.3",
    "vue-toast-notification": "^3.1.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^24.10.1",
    "@types/sortablejs": "^1.15.9",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.1",
    "husky": "^9.1.7",
    "json-server": "^0.17.4",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "sass": "^1.94.2",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.15",
    "vue-tsc": "^3.1.4"
  }
}
